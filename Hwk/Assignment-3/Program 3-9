// System Libraries
#include <iostream> // Input/Output Library

using namespace std; //Namespace of the System Libraries


//User Libraries



//Global Constants



//Function Prototypes



// Execution Begins Here!
int main(int argc, char** argv)
{
    //Declare Variables
    for (int count = 1; count < 5; count++)//Loop through five times
        cout << (2 * count) << " ";//write the multiples of two
    //END
  return 0;
} 

