// System Libraries
#include <iostream> // Input/Output Library

using namespace std; //Namespace of the System Libraries


//User Libraries



//Global Constants



//Function Prototypes



// Execution Begins Here!
int main(int argc, char** argv)
{
    //Declare Variables
    int x = 50;
    cout << "Start\n";
    if (x < 100)//if x is less than 100
    cout << "First Output.\n";
    else if (x > 100)//if x is more than 100
     cout << "Second Output.\n";
    else//if nothing elsedo this
     cout << x << endl;
    cout << "End\n";
    //END
  return 0;
} 

